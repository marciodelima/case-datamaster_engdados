{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond" : 5
      }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Information"
    }
  },
  "functionTimeout": "00:05:00",
  "retry": {
    "strategy": "fixedDelay",
    "maxRetryCount": 3,
    "delayInterval": "00:00:10"
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxOutstandingRequests": 100,
      "maxConcurrentRequests": 50
    },
    "timers": {
      "scheduleMonitor": {
        "enabled": true,
        "interval": "00:01:00"
      }
    }
  }
}

